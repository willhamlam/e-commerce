/*header*/ 
header
	position fixed
	width 100%
	background #ffffff

header .header-item
	width 18.5%
	float left
	font-size 12px
	margin 10px 0

header .header-item a
	display block
	width 100%
	color #000000

header .header-item.logo
	width 25%
	background #2d2d2d
	margin 0px
	padding-top 12px
	padding-bottom 8px

header .header-item.logo img
	width auto
	height 12px

.hb
	border-right 1px dotted #000000